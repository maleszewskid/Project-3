{
  "name": "project-3-server",
  "version": "1.0.0",
  "description": "Final Project Server",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start-prod || npm run start-dev",
    "start-prod": "node server.js",
    "start-dev": "concurrently \"nodemon --ignore 'app/*'\" \"npm run app\"",
    "client": "cd app && npm run start",
    "seed": "node seeds/seedDB.js",
    "install": "cd app && npm install",
    "build": "cd app && npm run build",
    "heroku-postbuild": "npm install -- prefix app && npm run build --prefix app"
  },
  "author": "Dom Maleszewski, Sam Levy, Jack Ryan, Phil Swartz",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.1",
    "concurrently": "^5.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.8.5",
    "morgan": "^1.9.1"
  }
}
